{
    "compilerOptions": {
      "jsx": "react",
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "~/*": ["./imports/*"]
      },
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["**/*.ts", "**/*.tsx", "client/main.js"],
    "exclude": ["node_modules"],
    "types": ["@testing-library/react"]
  }
  